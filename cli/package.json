{
    "name": "prompt-piper-cli",
    "version": "1.0.0",
    "description": "CLI tool for compressing AI prompts to reduce token usage",
    "main": "index.js",
    "bin": {
        "prompt-piper": "./bin/prompt-piper.js",
        "pp": "./bin/prompt-piper.js",
        "ppi": "./bin/prompt-piper-interactive.js"
    },
    "scripts": {
        "build": "tsc",
        "start": "node dist/index.js",
        "dev": "ts-node src/index.ts",
        "interactive": "ts-node src/interactive.ts",
        "interactive:dry": "ts-node src/interactive.ts --dry",
        "interactive:claude": "ts-node src/interactive.ts --claude",
        "interactive:openrouter": "ts-node src/interactive.ts --openrouter",
        "interactive:bitcoincom": "ts-node src/interactive.ts --bitcoincom",
        "test": "echo \"No tests yet\"",
        "compress": "bun run dev compress",
        "compress:file": "bun run dev compress -f example.txt",
        "compress:claude": "bun run dev compress --claude",
        "compress:openrouter": "bun run dev compress --openrouter",
        "compress:bitcoincom": "bun run dev compress --bitcoincom",
        "compress:bypass": "bun run dev compress --openrouter --bypass",
        "analyze": "bun run dev analyze",
        "analyze:file": "bun run dev analyze -f prompt.txt",
        "rules:list": "bun run dev rules:list",
        "example:simple": "echo 'Could you please help me understand how to implement a REST API?' | bun run dev compress",
        "example:verbose": "echo 'I would really appreciate if you could help me understand how machine learning algorithms work' | bun run dev compress",
        "example:stats": "echo 'Please provide a detailed step by step explanation' | bun run dev compress -s"
    },
    "keywords": [
        "ai",
        "prompt",
        "compression",
        "tokens",
        "cli",
        "claude",
        "cost-optimization"
    ],
    "author": "Prompt Piper Team",
    "license": "MIT",
    "dependencies": {
        "chalk": "^4.1.2",
        "commander": "^11.0.0",
        "dotenv": "^16.0.0",
        "js-tiktoken": "^1.0.21",
        "node-fetch": "^2.6.7",
        "ora": "^5.4.1",
        "thirdweb": "^5.0.0"
    },
    "devDependencies": {
        "@types/node": "^20.14.9",
        "@types/node-fetch": "^2.6.4",
        "ts-node": "^10.9.1",
        "typescript": "^5.5.3"
    },
    "files": [
        "dist/**/*",
        "bin/**/*",
        "README.md"
    ]
}
